<Window x:Class="面包管理系统.ProduceWin"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:面包管理系统"
        mc:Ignorable="d"
        Title="面包作坊" Height="175.61" Width="279.878" WindowStartupLocation="CenterOwner" Loaded="Window_Loaded" Icon="/面包管理系统;component/bread.png">
    <DockPanel>
        <ListView x:Name="typeList" Width="70" DockPanel.Dock="Left" HorizontalAlignment="Left" Margin="5"></ListView>
        <TextBox x:Name="breadCount" DockPanel.Dock="Left" Height="20" Width="40" HorizontalAlignment="Left" Margin="20,0,0,0" TextAlignment="Center">1</TextBox>
        <Button Click="Button_Click" Width="40" Height="30" DockPanel.Dock="Left" HorizontalAlignment="Left" Margin="20,0,0,0">确认</Button>
        <Button Click="Button_Click_1" Width="40" Height="30" DockPanel.Dock="Left" HorizontalAlignment="Left" Margin="20,0,0,0">取消</Button>
    </DockPanel>
</Window>
